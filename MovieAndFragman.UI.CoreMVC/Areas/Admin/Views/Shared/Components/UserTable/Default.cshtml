@model List<UserVM>

<table cellpadding="0" cellspacing="0" border="0" class="display table table-bordered" id="hidden-table-info">
    <thead>
        <tr>
            <th>Firstname</th>
            <th>Lastname</th>
            <th class="hidden-phone">Birthdate</th>
            <th class="hidden-phone">Email</th>
            <th class="hidden-phone">Phonenumber</th>
            <th style="display: none;"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (UserVM item in Model)
        {
<tr class="gradeA">
    <td>@item.FirstName</td>
    <td>@item.LastName</td>
    <td class="hidden-phone">@item.BrithDate</td>
    <td class="center hidden-phone">@item.Email</td>
    <td class="center hidden-phone">@item.PhoneNumber</td>
    <td style="display: none;">@item.UserName</td>
</tr>}
    </tbody>
</table>
